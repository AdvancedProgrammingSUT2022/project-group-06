<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="pane" prefHeight="650.0" prefWidth="1200.0" stylesheets="@../css/PanelsStyle.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.civilization.views.MapPage">
   <AnchorPane id="title" prefHeight="50.0" prefWidth="1200.0">
      <children>
      
                            <Button fx:id="saveGameButton" layoutX="14.0" layoutY="11.0" onMouseClicked="#saveGame" prefHeight="26.0" prefWidth="46.0" styleClass="gamepageButton" text="Save ">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Button>
          <Button fx:id="nextTurnButton" layoutX="70.0" layoutY="11.0" onMouseClicked="#nextTurn" styleClass="gamepageButton" text="Next Turn">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Button>
         <Button fx:id="notification" layoutX="718.0" layoutY="11.0" mnemonicParsing="false" onMouseClicked="#notification" styleClass="gamepageButton" text="Notification" />
         <Button layoutX="615.0" layoutY="11.0" mnemonicParsing="false" onMouseClicked="#demographic" styleClass="gamepageButton" text="Demographic" />
         <Button layoutX="554.0" layoutY="11.0" mnemonicParsing="false" onMouseClicked="#units" styleClass="gamepageButton" text="Units" />
      </children>
   </AnchorPane>

</Pane>
